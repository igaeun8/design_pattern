{
  "name": "design-pattern",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:factory": "vite --open entries/factory.html",
    "dev:adapter": "vite --open entries/adapter.html",
    "dev:decorator": "vite --open entries/decorator.html",
    "dev:observer": "vite --open entries/observer.html",
    "dev:singleton": "vite --open entries/singleton.html",
    "dev:visitor": "vite --open entries/visitor.html",
    "dev:prototype": "vite --open entries/prototype.html",
    "dev:composite": "vite --open entries/composite.html",
    "dev:command": "vite --open entries/command.html",
